model ordinateurs {
    id              Int       @id @default(autoincrement())
    macAddress      String    @unique @db.VarChar(17)
    marque          String
    modele          String
    dateAchat       DateTime
    dateFinGarantie DateTime?
    statut          String
    employeId       Int?
    employes         employe? @relation(fields: [employeId], references: [id])
    disponible      Boolean   @default(true)
}
